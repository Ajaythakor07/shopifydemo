<!doctype html>
<html>
<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Mukta:300,400,700"> 
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/icomoon@1.0.0/style.min.css"> 
  
  
  {{ content_for_header }} <!-- Header hook for plugins -->

  {{ 'aos.css' | asset_url | stylesheet_tag }}
  {{ 'icomoon.css' | asset_url | stylesheet_tag }}
  {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
  {{ 'magnific-popup.css' | asset_url | stylesheet_tag }}
  {{ 'jquery-ui.css' | asset_url | stylesheet_tag }}
  {{ 'owl.carousel.min.css' | asset_url | stylesheet_tag }}
  {{ 'owl.theme.default.min.css' | asset_url | stylesheet_tag }}
  {{ 'style.css' | asset_url | stylesheet_tag }}

  {{ 'jquery-3.3.1.min.js'| asset_url | script_tag }}  
  {{ 'jquery-ui.js'| asset_url | script_tag }}  
  {{ 'popper.min.js'| asset_url | script_tag }}  
  {{ 'bootstrap.min.js'| asset_url | script_tag }}  
  
  <script src="https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.3/owl.carousel.min.js"></script>
  {{ 'owl.carousel.min.js'| asset_url | script_tag }}
  {{ 'query.magnijfic-popup.min.jss'| asset_url | script_tag }}
  {{ 'main.js'| asset_url | script_tag }}
 

 
</head>
<body>  

 {% section 'header' %}
 

  <main>
    {{ content_for_layout }}
  </main>

  {% section 'footer' %}

  {{ content_for_footer }}

</body>
</html>
